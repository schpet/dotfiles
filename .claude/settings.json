{
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Edit|MultiEdit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/schpet/.cargo/bin/jjagent claude hooks PostToolUse"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Edit|MultiEdit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/schpet/.cargo/bin/jjagent claude hooks PreToolUse"
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/schpet/code/dotfiles/.claude/block-git-writes.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/schpet/.cargo/bin/jjagent claude hooks Stop"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "/Users/schpet/code/dotfiles/.claude/statusline.sh"
  },
  "alwaysThinkingEnabled": false,
  "defaultMode": "edit"
}
