{
  "vim.statusBarColorControl": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascriptreact]": { "editor.defaultFormatter": "biomejs.biome" },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.trimTrailingWhitespace": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 100,
    "editor.rulers": [100]
  },
  "[ruby]": {
    "editor.formatOnSave": false,
    "editor.rulers": [120],
    "editor.defaultFormatter": "Shopify.ruby-lsp"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.minimap.enabled": false,
  "editor.rulers": [80],
  "editor.wordBasedSuggestions": "off",
  "emmet.excludeLanguages": ["js", "jsx", "ts", "tsx"],
  "eslint.validate": ["javascript", "javascriptreact"],
  "explorer.confirmDragAndDrop": false,
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.suggest.completeJSDocs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "ruby.format": "rubyfmt",
  "ruby.intellisense": "rubyLocate",
  "ruby.lint": {
    "standard": true,
    "rubocop": false
  },
  "terminal.integrated.fontFamily": "FiraCode-Retina",
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontWeight": "300",
  "typescript.autoClosingTags": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.handleKeys": {},
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<leader>", "."],
      "commands": ["alternate.alternateFile"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.inlineSuggest.trigger"]
    },
    {
      "before": ["<leader>", "c", "r", "c"],
      "commands": ["extension.changeCase.camel"]
    },
    {
      "before": ["<leader>", "c", "r", "p"],
      "commands": ["extension.changeCase.pascal"]
    },
    {
      "before": ["<leader>", "c", "r", "s"],
      "commands": ["extension.changeCase.snake"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "["],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "rubyfmt"
        }
      ]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["tromp.runPreviousCommand"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["tromp.runCommandWithNearest"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["tromp.runCommandWithFile"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["tromp.runCommand"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["tromp.openBookmark"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    // {
    //   "before": ["%"],
    //   "commands": [
    //     {
    //       "command": "editor.action.jumpToBracket"
    //     }
    //   ]
    // },
    {
      "before": ["&"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["linear-open-issue.openIssue"]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    }
  ],
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  // "workbench.colorCustomizations": {
  //   "focusBorder": "#ff9100",
  //   "statusBar.background": "#555555",
  //   "editor.lineHighlightBackground": "#111111",
  //   "tab.activeBackground": "#523500",
  //   "list.focusBackground": "#3F4045"
  // },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.accessibilitySupport": "off",
  "ruby.useLanguageServer": true,
  "editor.fontSize": 13,
  "editor.trimAutoWhitespace": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "diffEditor.codeLens": true,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.wordWrap": "on",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "vscode-graphql.showExecCodelens": false,
  "editor.semanticHighlighting.enabled": true,
  "[prisma]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.scrollback": 5000,
  "workbench.panel.opensMaximized": "always",
  "editor.find.seedSearchStringFromSelection": "never",
  "terminal.integrated.fontSize": 13,
  "editor.fontLigatures": true,
  "git.autofetch": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "javascript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "editor.snippetSuggestions": "bottom",
  "zenMode.fullScreen": false,
  "zenMode.centerLayout": false,
  "files.insertFinalNewline": true,
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "*.async": {},
      "enum": {
        "underline": true
      },
      "type": {
        "italic": true
      }
    }
  },
  "githubPullRequests.pullBranch": "never",
  "redhat.telemetry.enabled": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "styco.objectSyntax": true,
  "editor.codeActionsOnSave": {
    // "source.organizeImports": true
    // "quickfix.rome": true
  },
  "css.enabledLanguages": ["html", "erb", "ruby"],
  "railsI18n.loadAllTranslations": true,
  "markdown.validate.enabled": true,
  "tailwindCSS.colorDecorators": false,
  "files.associations": {
    "*.inky": "erb"
  },
  "vscode-erb-beautify.useBundler": false,
  "workbench.preferredLightColorTheme": "Solarized Light",
  "window.autoDetectColorScheme": true,
  "terminal.integrated.fontWeight": "normal",
  "rust-analyzer.lens.enable": false,
  "[rust]": {
    "editor.inlayHints.enabled": "offUnlessPressed"
  },
  "errorLens.enabledDiagnosticLevels": ["error", "warning"],
  // "editor.inlineSuggest.enabled": false,
  "editor.tabSize": 2,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "Cargo.toml": "Cargo.lock",
    "*.ts": "${capture}.typegen.ts",
    "*.tsx": "${capture}.typegen.ts",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "yard.spacers.beforeDescription": false,
  "yard.spacers.afterTags": false,
  "yard.spacers.separateTags": false,
  "css.styleSheets": [],
  "github.copilot.advanced": {},
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "rubyLsp.rubyVersionManager": {
    "identifier": "chruby"
  },
  "rubyLsp.formatter": "rubyfmt",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "git.openRepositoryInParentFolders": "never",
  "window.zoomLevel": -1,
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "workbench.colorTheme": "Solarized Light",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "openInDesktopApp": true,
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "workbench.preferredHighContrastLightColorTheme": "Solarized Light",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "workbench.preferredHighContrastColorTheme": "Default Dark+",
  "terminal.integrated.macOptionIsMeta": true,
  "continue.pauseTabAutocompleteOnBattery": true,
  "biome.enabled": false
}
