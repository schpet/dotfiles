{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[ruby]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.minimap.enabled": false,
  "editor.rulers": [80],
  "editor.wordBasedSuggestions": false,
  "emmet.excludeLanguages": ["js", "jsx", "ts", "tsx"],
  "eslint.validate": ["javascript", "javascriptreact"],
  "explorer.confirmDragAndDrop": false,
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.suggest.completeJSDocs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "ruby.format": "standard",
  "ruby.intellisense": "rubyLocate",
  "ruby.lint": {
    "standard": true,
    "rubocop": false
  },
  "terminal.integrated.fontFamily": "FiraCode-Retina",
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontWeight": "300",
  "typescript.autoClosingTags": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.handleKeys": {},
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<leader>", "."],
      "commands": ["alternate.alternateFile"]
    },
    {
      "before": ["<leader>", "c", "r", "c"],
      "commands": ["extension.changeCase.camel"]
    },
    {
      "before": ["<leader>", "c", "r", "p"],
      "commands": ["extension.changeCase.pascal"]
    },
    {
      "before": ["<leader>", "c", "r", "s"],
      "commands": ["extension.changeCase.snake"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["tromp.runPreviousCommand"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["tromp.runCommandWithNearest"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["tromp.runCommandWithFile"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["tromp.runCommand"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["tromp.openBookmark"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["%"],
      "commands": [
        {
          "command": "editor.action.jumpToBracket"
        }
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    }
  ],
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "workbench.colorCustomizations": {
    "focusBorder": "#ff9100",
    "statusBar.background": "#555555",
    "editor.lineHighlightBackground": "#111111",
    "tab.activeBackground": "#523500",
    "list.focusBackground": "#3F4045"
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.accessibilitySupport": "off",
  "ruby.useLanguageServer": true,
  "editor.fontSize": 13,
  "editor.trimAutoWhitespace": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "diffEditor.codeLens": true,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.wordWrap": "off",
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "vscode-graphql.showExecCodelens": false,
  "editor.semanticHighlighting.enabled": true,
  "[prisma]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.scrollback": 5000,
  "workbench.panel.opensMaximized": "always",
  "editor.find.seedSearchStringFromSelection": "never",
  "terminal.integrated.fontSize": 13,
  "editor.fontLigatures": true,
  "git.autofetch": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "javascript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "editor.snippetSuggestions": "bottom",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "zenMode.fullScreen": false,
  "zenMode.centerLayout": false,
  "zenMode.hideTabs": false,
  "files.insertFinalNewline": true,
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "*.async": {},
      "enum": {
        "underline": true
      },
      "type": {
        "italic": true
      }
    }
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "githubPullRequests.pullBranch": "never",
  "[toml]": {
    "editor.defaultFormatter": "bodil.prettier-toml"
  },
  "redhat.telemetry.enabled": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[erb]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
  },
  "styco.objectSyntax": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  }
}
