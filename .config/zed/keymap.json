[
  {
    "bindings": {
      "ctrl-j": "workspace::ActivateNextPane"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "space f": "editor::CopyRelativePath",
      "space p": "editor::Format",
      "space b": "editor::ToggleGitBlame"
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "shift-y": ["workspace::SendKeystrokes", "y $"]
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-n": "workspace::NewTerminal"
    }
  }
]
